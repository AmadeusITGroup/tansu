{
  "name": "ngx-tansu",
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/platform-browser": "^11.1.1",
    "@angular/platform-browser-dynamic": "^11.1.1",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@microsoft/api-documenter": "^7.11.3",
    "@microsoft/api-extractor": "^7.12.0",
    "@types/jasmine": "~3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-selenium-launcher": "^1.0.1",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.1",
    "rxjs": "^6.6.3",
    "typescript": "^4.1.3",
    "zone.js": "^0.10.3"
  },
  "scripts": {
    "test": "ng test -c ci",
    "test-tdd": "ng test",
    "clean": "rm -rf dist temp",
    "lint": "eslint src/{,**/}*.ts",
    "prebuild": "yarn clean",
    "build": "tsc",
    "check-format": "prettier --check .",
    "api": "api-extractor run",
    "docs": "api-documenter markdown -i temp -o dist/docs",
    "docs-publish": "gh-pages -d dist/docs",
    "package": "cp README.md src/package.json src/.npmignore dist/src"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
